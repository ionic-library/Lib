    "ionic-angular": "3.6.1",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "rtcmulticonnection-v3": "3.4.4",
    "rxjs": "5.4.0",
    "simple-peer": "8.1.1",
    "splash-screen": "^4.0.1",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12",
    "cordova-android": "~6.3.0"
  },
  "devDependencies": {
    "@ionic/app-scripts": "2.1.4",
    "ionic": "3.19.0",
    "typescript": "2.3.4"
