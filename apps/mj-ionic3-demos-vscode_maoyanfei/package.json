    "@ionic-native/camera": "^4.2.1",
    "@ionic-native/core": "^4.2.1",
    "@ionic-native/in-app-browser": "^4.2.1",
    "@ionic-native/splash-screen": "^4.2.1",
    "@ionic-native/status-bar": "^4.2.1",
